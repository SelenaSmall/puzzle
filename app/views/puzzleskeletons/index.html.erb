<p id="notice"><%= notice %></p>

<h1>Listing Puzzleskeletons</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @puzzleskeletons.each do |puzzleskeleton| %>
      <tr>
        <td><%= puzzleskeleton.name %></td>
        <td><%= link_to 'Show', puzzleskeleton %></td>
        <td><%= link_to 'Edit', edit_puzzleskeleton_path(puzzleskeleton) %></td>
        <td><%= link_to 'Destroy', puzzleskeleton, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Puzzleskeleton', new_puzzleskeleton_path %>
